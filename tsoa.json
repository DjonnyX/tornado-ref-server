{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "securityDefinitions": {
      "clientAccessToken": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      },
      "terminalAccessToken": {
        "type": "apiKey",
        "name": "x-access-token",
        "in": "header"
      }
    },
    "basePath": "/api/v1",
    "outputDirectory": ".",
    "specVersion": 3,
    "tags": [
      {
        "name": "Tag",
        "description": "Operations about tags",
        "externalDocs": {
          "description": "Find out more about tags",
          "url": "http://swagger.io"
        }
      }
    ],
    "paths": {
      "/assets/upload": {
        "post": {
          "consumes": [
            "multipart/form-data"
          ],
          "parameters": [
            {
              "in": "formData",
              "name": "file",
              "required": true,
              "type": "file"
            }
          ]
        }
      }
    }
  },
  "routes": {
    "basePath": "/api/v1",
    "routesDir": "src",
    "authenticationModule": "./src/authentication.ts"
  }
}